@model Ecommerce__projet.Models.Client

@{
    ViewBag.Title = "Client";
}


<h2>Choix du client</h2>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
<div style="float:center">
    @Html.DropDownList("listeclient", ViewBag.listvendeur as SelectList, "Choisissez un identifiant")
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Create" class="btn btn-default" />
        </div>
    </div>
    <li><a class="btn btn-success">@Html.ActionLink("Creer un Client", "InscrireClientt", "Client")</a></li>


    }
</div>


